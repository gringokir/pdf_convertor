<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.company.MainSceneController">

    <HBox alignment="CENTER" prefHeight="200.0" prefWidth="300.0" spacing="10.0">
        <Button fx:id="fromJPGtoPDF" alignment="CENTER" mnemonicParsing="false" onAction="#JPGtoPDF" text="Из JPG в PDF" />
        <Button fx:id="fromPDFtoJPG" alignment="CENTER" mnemonicParsing="false" onAction="#PDFtoJPG" text="Из PDF в JPG" />
    </HBox>

</VBox>
